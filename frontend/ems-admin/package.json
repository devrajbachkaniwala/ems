{
  "name": "ems-admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 4001",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "schema:download": "npx apollo service:download --endpoint=http://localhost:3001/graphql ./app/graphql/graphql-schema.json",
