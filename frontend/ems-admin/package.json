  },
  "dependencies": {
    "@apollo/client": "^3.5.8",
    "axios": "^0.25.0",
    "graphql": "^16.3.0",
    "jsonwebtoken": "^8.5.1",
    "mobx": "^6.4.2",
    "mobx-react-lite": "^3.3.0",
    "next": "12.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.3.1"
  },
  "devDependencies": {
    "@types/graphql": "^14.5.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "17.0.21",
    "@types/react": "17.0.40",
    "autoprefixer": "^10.4.2",
    "eslint": "8.11.0",
    "eslint-config-next": "12.1.0",
    "postcss": "^8.4.8",
    "tailwindcss": "^3.0.23",
    "typescript": "4.6.2"
  }
